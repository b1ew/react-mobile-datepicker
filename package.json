    "styled-loader": "^0.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
